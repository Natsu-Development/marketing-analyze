                },
                "url": {
                  "raw": "{{baseUrl}}/api/v1/auth/facebook/session",
                  "host": ["{{baseUrl}}"],
                  "path": ["api", "v1", "auth", "facebook", "session"]
                }
              },
              "status": "OK",
              "code": 200,
