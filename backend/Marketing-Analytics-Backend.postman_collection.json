        {
          "name": "OAuth Callback",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/facebook/callback?code=AUTH_CODE&state=STATE_STRING",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "facebook", "callback"],
              "query": [
                {
                  "key": "code",
                  "value": "AUTH_CODE",
                  "description": "Authorization code from Facebook"
